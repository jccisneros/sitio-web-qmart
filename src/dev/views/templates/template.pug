block config
    include ../config/config.pug
doctype html
html
    head
        title #{pageTitle} / #{siteTitle}
        meta(http-equiv="Content-Type", content="text/html;charset=UTF-8")
        meta(name='description' content = description)
        link(rel="stylesheet", href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous")
        link(rel="stylesheet", href="./css/bootstrap-material-design.min.css")
        link(rel="stylesheet", href="./css/main.css")
        <link rel="stylesheet" href="./css/owl.carousel.min.css">
        <link rel="stylesheet" href="./css/owl.theme.default.min.css">
        <script src="./js/jquery.min.js"></script>
        <script src="./js/owl.carousel.min.js"></script>
    body
        menu            
            include ../blocks/menu.pug

        main
            include ../blocks/header.pug
            include ../blocks/somos.pug
            include ../blocks/servicios.pug
            include ../blocks/elegirnos.pug
            include ../blocks/portafolio.pug
            include ../blocks/contacto.pug
            include ../blocks/instagram.pug
            include ../blocks/footer.pug
            
            
        footer            
        
        script(src="./js/popper.min.js")        
        script(src="./js/bootstrap-material-design.min.js")
        script(src="./js/main.js")
        script.
            $(document).ready(function(){
                $(".owl-carousel").owlCarousel();
            });        